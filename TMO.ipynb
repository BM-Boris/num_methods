        "\n",
        "import numpy\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy.random as npr\n",
        "import math\n",
        "\n",
        "def wait():\n",
        "    Twait=0\n",
        "    t=0\n",
        "    E=0\n",
        "    count=0\n",
        "    line=0\n",
        "    u=5\n",
        "    l=8\n",
        "    n=2\n",
        "\n",
        "    time =[]\n",
        "    c = []\n",
        "    while t<8:\n",
        "        \n",
        "        mi=min(count,n)\n",
        "        E=-math.log(npr.rand())/(l+mi*u)\n",
        "          \n",
        "        t=t+E\n",
        "        if t<8:\n",
        "          time.append(t)\n",
        "          c.append(count)\n",
        "          \n",
        "        if count>n:\n",
        "          Twait+=(count-n)*E\n",
        "        if npr.rand()<mi*u/(l+mi*u):\n",
        "            count-=1\n",
        "        else:\n",
        "            count+=1 \n",
        "            line+=1 \n",
        "    plt.step(time, c)\n",
        "    return Twait/line\n",
        "\n",
        "wait()"
      ],
